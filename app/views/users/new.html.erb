
<h1>Registration</h1>

<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :bio %>
  <%= f.text_area :bio %>

  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>

  <%=  %>
  <%= f.submit %>
<% end %>
