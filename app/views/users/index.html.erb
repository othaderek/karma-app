
<h1>Find A User</h1>
<br>
<div class="user-index">

  <div class="section-2">
    <%= form_tag users_path, method: "GET" %>
      <%= text_field_tag :search %>
      <%= submit_tag "Search" %>
  </div>
  <br>
  <div class="section">
    <ul>
      <% @users.each do |user| %>
        <li>
          <%= link_to user.username, user %>
        </li>
      <% end %>
    </ul>
  </div>

</div>
